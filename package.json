{
  "name": "yallma3-cli",
  "version": "1.0.0",
  "description": "AI Agent Development Environment - Terminal UI",
  "type": "module",
  "bin": {
    "yallma3": "./dist/index-tui.js"
  },
  "scripts": {
    "dev": "tsx src/index-tui.tsx",
    "build": "tsc && chmod +x dist/index-tui.js",
    "start": "node dist/index-tui.js",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "format": "prettier --write src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/crypto-js": "^4.2.2",
    "chalk": "^5.3.0",
    "crypto-js": "^4.2.0",
    "ink": "^4.4.1",
    "ink-select-input": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "meow": "^13.0.0",
    "react": "^18.2.0",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
